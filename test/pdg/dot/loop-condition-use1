digraph {
	0[shape=ellipse,label="[0] ENTRY"]
	1[shape=ellipse,label="[1] int x = 1"]
	2[shape=ellipse,label="[2] x < 10"]
	3[shape=ellipse,label="[3] System.out.println(\"loop\")"]
	4[shape=ellipse,label="[4] x = x + 1"]
	5[shape=ellipse,label="[5] System.out.println(\"x = \" + x)"]
	0 -> 1 [label="CONTROL:T"]
	0 -> 2 [label="CONTROL:T"]
	0 -> 5 [label="CONTROL:T"]
	1 -> 2 [label="DATA:x"]
	1 -> 4 [label="DATA:x"]
	1 -> 5 [label="DATA:x"]
	2 -> 2 [label="CONTROL:T"]
	2 -> 3 [label="CONTROL:T"]
	2 -> 4 [label="CONTROL:T"]
	4 -> 2 [label="DATA:x"]
	4 -> 4 [label="DATA:x"]
	4 -> 5 [label="DATA:x"]
}
