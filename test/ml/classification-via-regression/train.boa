type T = enum {y = "true", n = "false"};
label: T;
p: Project = input;

cvr : output classificationviaregression(
	"-s 75 "
	 + " -W weka.classifiers.rules.J48"
	 + " -W weka.classifiers.rules.M5P"
	 + " -W weka.classifiers.rules.RandomForest"
) of {int, T, int, T};

i := 0;
while(i < 100){

	label = T.y;
	if (i % 2 == 0)
		label = T.n;

	x1 := i + 7;
	x2: T = T.n;
	x3 := i;

	cvr << {x1, x2, x3, label};
	i++;
}