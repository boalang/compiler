p: Project = input;
kmeans: output simplekmeans(
	"-N 3 -V "			# other options
	+ "-s 75 "			# 75% as training data
	+ "-A "				# attribute names
	+ "outlook:"		# attribute1
	+ "temperature:"	# attribute2
	+ "humidity:"		# attribute3
	+ "windy:"			# attribute4
	+ "play"			# attribute5
) of array of string;

kmeans << {"sunny","85","85","FALSE","no"};
kmeans << {"sunny","80","90","TRUE","no"};
kmeans << {"overcast","83","86","FALSE","yes"};
kmeans << {"rainy","70","96","FALSE","yes"};
kmeans << {"rainy","68","80","FALSE","yes"};
kmeans << {"rainy","65","70","TRUE","no"};
kmeans << {"overcast","64","65","TRUE","yes"};
kmeans << {"sunny","72","95","FALSE","no"};
kmeans << {"sunny","69","70","FALSE","yes"};
kmeans << {"rainy","75","80","FALSE","yes"};
kmeans << {"sunny","75","70","TRUE","yes"};
kmeans << {"overcast","72","90","TRUE","yes"};
kmeans << {"overcast","81","75","FALSE","yes"};
